@InProceedings{zhao,
author="Zhao, Xinchao
and Guan, Hongtu
and Sun, Hao
and Lu, Juan",
editor="Zhang, Zhenyuan",
title="A Prophet-Based Passenger Flow Prediction Model on IC Card Data",
booktitle="2021 6th International Conference on Intelligent Transportation Engineering (ICITE 2021)",
year="2022",
publisher="Springer Nature Singapore",
address="Singapore",
pages="1082--1092",
abstract="At present, there is a widespread conflict in China's transportation industry that people's travel needs and the transportation capacity of the bus industry do not match. In order to solve the imbalance of public bus passenger flow during the peak period of urban residents in China. It is proposed to take urban public transportation IC card data as the research object, comprehensively consider the impact of weather, temperature and other objective factors, and use the prophet time prediction sequence model to establish a public transit passenger flow prediction model at various periods of time, then, according to the passenger flow forecasting model, scientifically make bus route schedules to improve passenger comfort and meet passenger travel needs. Finally, the real-time smart IC card data of Zhengzhou Public Transport was used for comparison and verification, which proved the accuracy of the prophet-based passenger flow prediction model for public transit time, and realized the adjustment of the bus capacity by predicting the bus passenger flow to alleviate the problem of urban residents' travel congestion.",
isbn="978-981-19-2259-6"
}
